{
  "manifest_version": 3,
  "name": "Github Repository Tools",
  "version": "0.1.0",
  "description": "My Chrome Extension",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icons/icon.png"
  },
  "permissions": [
    "scripting",
    "webNavigation"
  ],
  "host_permissions": ["https://github.com/*"]
}